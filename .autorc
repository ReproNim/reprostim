{
    "onlyPublishWithReleaseLabel": true,
    "baseBranch": "master",
    "author": "ReproNim Team[bot] <info@repronim.org>",
    "noVersionPrefix": true,
    "plugins": [
        "git-tag",
        [
            "exec",
            {
                  "afterRelease": [
                      "python -m build && twine upload dist/*",
                      "bash tools/ci/release_reprostim_docker.sh"
                  ]
            }
        ],
        "released"
    ]
}
